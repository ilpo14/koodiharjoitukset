def select_long_words(words)
	over4words = []
  
  i = 0
  
  	while i < words.length
      ogword = words[i]
      if ogword.length > 4
        over4words << ogword
      end
    i += 1
    end
  return over4words
end

print select_long_words(["what", "are", "we", "eating", "for", "dinner"]) # => ["eating", "dinner"]
puts
print select_long_words(["keep", "coding"])   
